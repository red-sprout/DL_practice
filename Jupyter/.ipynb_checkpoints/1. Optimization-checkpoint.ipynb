   "source": [
    "import numpy as np\n",
    "\n",
    "H = np.matrix([[2,0],[0,2]])\n",
    "g = -np.matrix([[6],[6]])\n",
    "\n",
    "x = np.zeros((2,1))\n",
    "alpha = 0.2\n",
    "\n",
    "for i in range(25):\n",
    "    df = H*x + g\n",
    "    x = x - alpha*df\n",
    "\n",
    "print(x)"
   ]
